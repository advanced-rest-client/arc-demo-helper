{
  "name": "@advanced-rest-client/arc-demo-helper",
  "version": "1.0.5",
  "description": "An element to help create demo pages for ARC components",
  "keywords": [
    "web-components",
    "advanced-rest-client",
    "api-components",
    "demo"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/arc-demo-helper.git"
  },
  "license": "Apache-2.0",
  "main": "src/arc-demo-helper.js",
  "module": "src/arc-demo-helper.js",
  "authors": [
    "pawel Psztyc",
    "The Advanced REST client authors <arc@mulesoft.com>"
  ],
  "homepage": "https://github.com/advanced-rest-client/arc-demo-helper",
  "scripts": {
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:legacy": "karma start --legacy --coverage",
    "test:legacy:watch": "karma start --legacy --auto-watch=true --single-run=false",
    "update-types": "gen-typescript-declarations --deleteExisting --outDir ."
  },
  "dependencies": {
    "@api-components/raml-aware": "^3.0.0",
    "@polymer/font-roboto": "^3.0.2",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-item": "^3.0.0",
    "@polymer/paper-listbox": "^3.0.0",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.1",
    "prismjs": "^1.17.1",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.2.1",
    "@open-wc/testing-karma": "^3.1.5",
    "@polymer/gen-typescript-declarations": "^1.6.2",
    "es-dev-server": "^1.8.3",
    "deepmerge": "^4.0.0"
  }
}
