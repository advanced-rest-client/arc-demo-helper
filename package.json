    "@polymer/gen-typescript-declarations": "^1.6.2",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "typescript-lit-html-plugin": "^0.9.0"
