    "@anypoint-web-components/anypoint-switch": "^0.1.3",
    "@anypoint-web-components/anypoint-tabs": "^0.1.10",
    "@api-components/amf-helper-mixin": "^4.0.26",
    "@api-components/api-navigation": "^4.1.0",
    "@api-components/raml-aware": "^3.0.0",
